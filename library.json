{
	"name": "canfestival",
	"keywords":"can, canopen, canfestival",
	"description":"CanFestival fork",
	"homepage":"https://canfestival.org/",
	"authors":
	{
		"name":"ljessen",
		"email":"github@ljessen.dk",
		"url":"https://github.com/ljessendk/CanFestival"
	},
	"repository":
	{
		"type":"git",
		"url":"https://github.com/theeuwke/CanFestival"
	},
	"export": {
		"include":
		[
			"src/*.c",
			"drivers/cm3/*.c"
		],
		"exclude":
		[
			"debian",
			"doc",
			"examples",
			"objdictgen",
			"*.vcproj",
			"*.sln"
		]
	},
	"frameworks": "*",
	"platforms": "*",
	"examples": [
		"[Ee]xamples/*.c",
		"[Ee]xamples/*.h",
		"[Ee]xamples/*.od",
		"[Ee]xamples/*.vxproj",
		"[Ee]xamples/*.filters",
		"[Ee]xamples/*.s"
	],
	"build": {
		"flags": [
			"-DENABLE_SHARED",
			"-Iinclude",
			"-Idrivers/cm4"
		],
		"src_filter": [
			"+<src/>",
			"+<drivers/cm3"
		]
	}
}
